<p id="notice"><%= notice %></p>

<h1>Establishments</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Title</th>
      <th>Address</th>
      <th>Npa</th>
      <th>Locality</th>
      <th>Email</th>
      <th>Phone1</th>
      <th>Phone2</th>
      <th>Fax</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @establishments.each do |establishment| %>
      <tr>
        <td><%= establishment.code %></td>
        <td><%= establishment.title %></td>
        <td><%= establishment.address %></td>
        <td><%= establishment.npa %></td>
        <td><%= establishment.locality %></td>
        <td><%= establishment.email %></td>
        <td><%= establishment.phone1 %></td>
        <td><%= establishment.phone2 %></td>
        <td><%= establishment.fax %></td>
        <td><%= link_to 'Show', establishment %></td>
        <td><%= link_to 'Edit', edit_establishment_path(establishment) %></td>
        <td><%= link_to 'Destroy', establishment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Establishment', new_establishment_path %>
